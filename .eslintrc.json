{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": "true"
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/label-has-for": "0"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.common.js"
        }
      }
    }
}
